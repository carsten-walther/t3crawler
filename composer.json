{
	"name": "walther/t3crawler",
	"type": "typo3-cms-extension",
	"description": "A wrapper for crawler to let TYPO3 CLI run recursivly through sitemap.xml.",
	"keywords": [
		"TYPO3",
		"extension",
		"crawler",
		"sitemap"
	],
	"homepage": "https://carstenwalther.de",
	"license": "GPL-3.0-or-later",
	"require": {
		"typo3/cms-core": "^9.5 || ^11.5"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "t3crawler"
		}
	},
	"autoload" : {
		"psr-4" : {
			"Walther\\T3crawler\\" : "Classes/"
		}
	},
	"minimum-stability" : "beta",
	"authors" : [{
		"name" : "Carsten Walther",
		"email" : "walther.carsten@web.de"
	}]
}
